{
    "name": "madebymouses/magento2-store-pickup",
    "description": "Store Pickup module for Magento 2",
    "type": "magento2-module",
    "license": "MIT",
    "autoload": {
        "files": [
            "src/registration.php"
        ],
        "psr-4": {
            "MadeByMouses\\StorePickup\\": "src/"
        }
    },
    "require": {
        "php": "^8.1",
        "magento/framework": "103.0.*",
        "magento/module-quote": "101.2.*",
        "magento/module-shipping": "100.4.*"
    },
    "archive": {
        "exclude": [
            "/.gitignore"
        ]
    },
    "config": {
        "sort-packages": true
    }
}
